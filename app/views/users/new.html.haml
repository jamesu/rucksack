- @page_title = t('create_user')
- @tabbed_navigation_items = common_tabs(nil)
- @user_navigation_items = user_tabs(:users)

#userForm
  = form_with(model: @user, url:users_path(), method: :post) do |form|
    = render partial: 'form', locals: {form: form}
    %button.submit{type: 'submit'}= t('save')
    %a.cancel{href: users_path}= t('cancel')
